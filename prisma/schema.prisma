generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  
  url      = env("DB_URI")
}

model Car {
  id             Int     @id @default(autoincrement())
  city_mpg       Int
  class          String
  combination_mpg Int
  cylinders      Int
  displacement   Float
  drive          String
  fuel_type      String
  highway_mpg    Int
  make           String
  model          String
  transmission   String
  year           Int
  imageUrls       String[]  @default([])
}



